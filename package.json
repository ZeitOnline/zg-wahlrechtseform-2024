    "react-intersection-observer": "^9.10.2",
    "react-map-gl": "^7.1.7",
    "react-router-dom": "^6.23.1",
    "react-select": "^5.8.0",
    "react-spring": "^9.7.3",
    "react-toggle": "^4.1.3",

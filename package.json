{
  "name": "zg-starterkit",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node --watch starterkit/vite/server.js",
    "copy:content:from:vivi": "node starterkit/scripts/copy-content-from-vivi.mjs index && npx prettier --write src/apps/index/pages/index.jsx",
    "copy:content:to:vivi": "node starterkit/scripts/copy-content-to-vivi.mjs index",
    "build": "npm run build:client && npm run build:node && npm run build:embeds",
    "build:node": "SSRBUILD=true vite build --config starterkit/vite/config/vite.config.js",
    "build:client": "vite build --config starterkit/vite/config/vite.config.js",
    "build:embeds": "NODE_NO_WARNINGS=1 NODE_ENV=production node starterkit/deployment/index.js",
    "build:templates": "node starterkit/scripts/build-templates.js",
    "build:maptiles": "sh maptiles/create.sh && node maptiles/analyse.js",
    "build:storybook": "npm run build:stories && NODE_OPTIONS=--openssl-legacy-provider storybook build",
    "build:tokens": "cd ./core/styles/tokens && node build-tokens.js",
    "build:docs": "npx jsdoc-to-markdown starterkit/vivi/parameter-types.js > starterkit/vivi/parameter-types.md",
    "build:stories": "node starterkit/scripts/create-stories-for-md.mjs",
    "map:create-tiles": "sh maptiles/create.sh && node maptiles/analyse.js",
    "map:create-rastertiles": "sh maptiles_raster/create.sh",
    "deploy:static": "starterkit/scripts/deploy.sh",
    "deploy:vivi": "node starterkit/scripts/deploy-to-vivi.js",
    "deploy": "npm run deploy:static && npm run deploy:vivi",
    "deploy:storybook": "starterkit/scripts/deploy-storybook.sh",
    "publish": "npm run build && npm run deploy:static && npm run deploy:vivi",
    "publish:staging": "STAGING=true npm run build && STAGING=true npm run deploy:static && STAGING=true npm run deploy:vivi",
    "publish:storybook": "npm run build:storybook && npm run deploy:storybook",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider npm run build:docs && npm run build:stories && NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 8000",
    "html2pembed": "node core/scripts/html2pembed/index.js"
  },
  "dependencies": {
    "@floating-ui/react-dom-interactions": "^0.13.3",
    "@mapbox/geojson-rewind": "^0.5.2",
    "@mapbox/sphericalmercator": "^1.2.0",
    "@mdx-js/rollup": "^3.0.1",
    "@react-hook/resize-observer": "^1.2.6",
    "@storybook/addon-storysource": "^8.0.0",

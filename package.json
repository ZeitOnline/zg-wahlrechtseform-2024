    "@use-gesture/react": "^10.3.1",
    "browserslist": "^4.23.0",
    "change-case": "^5.4.4",
    "cheerio": "^1.0.0-rc.12",
    "classnames": "^2.5.0",
    "commander": "^10.0.0",
    "d3-dsv": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-geo": "^3.1.1",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "d3-time-format": "^4.1.0",
    "date-fns": "^2.30.0",
    "debounce": "^1.2.1",
